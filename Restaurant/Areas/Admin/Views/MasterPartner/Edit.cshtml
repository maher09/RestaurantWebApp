@model Restaurant.Areas.Admin.ViewModel.MVMasterPartner

@{
    ViewData["Title"] = "Edit";
}


<div class="pagetitle">
    <h1>Master Partner</h1>

    <nav>

        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Home</a></li>
            <li class="breadcrumb-item"><a asp-action="Index" asp-controller="MasterPartner">Master Partner</a></li>

            <li class="breadcrumb-item active">Edit</li>
        </ol>

    </nav>

</div>

<section class="section">
    <div class="row">
        <div class="col-lg-12">


            <div class="card">
                <div class="card-body">
                    <h5 class="card-title m-0"></h5>
                    <form asp-action="Edit" class="row g-3">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                        <div class="col-md-6">
                            <label asp-for="MasterPartnerName" class="control-label"></label>
                            <input asp-for="MasterPartnerName" class="form-control" />
                            <span asp-validation-for="MasterPartnerName" class="text-danger"></span>

                        </div>
                     

                        <div class="col-md-6">
                            <label asp-for="MasterPartnerWebsiteUrl" class="control-label"></label>
                            <input asp-for="MasterPartnerWebsiteUrl" class="form-control" />
                            <span asp-validation-for="MasterPartnerWebsiteUrl" class="text-danger"></span>
                            </div>

                        <div class="col-6">
                                <label asp-for="File" class="control-label"></label>
                                <input asp-for="File" class="form-control" />
                                <span asp-validation-for="File" class="text-danger"></span>

                        </div>



                            <div class="form-group text-end">

                                <a asp-action="Index" class="btn btn-success">Back</a>
                                <input type="submit" value="Submit" class="btn btn-primary" />

                            </div>
                    </form>

                </div>
            </div>
        </div>

    </div>
</section>






